{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dev Light",
  "type": "light",
  "colors": {
    "editor.background": "#F0F7F5",
    "editor.foreground": "#3F4642",
    "editorLineNumber.foreground": "#B0BAB5",
    "editorLineNumber.activeForeground": "#17C7A4",
    "editorCursor.foreground": "#FF5D70",
    "editor.selectionBackground": "#E7F2EE",
    "editor.inactiveSelectionBackground": "#EFF7F3",
    "editor.lineHighlightBackground": "#F0F7F5",
    "editorWhitespace.foreground": "#C5D5C9",
    "editorIndentGuide.background1": "#C5D5C9",
    "editorIndentGuide.activeBackground1": "#6E7972",
    "activityBar.background": "#F0F7F5",
    "activityBar.foreground": "#FF5D70",
    "activityBarBadge.background": "#20C093",
    "activityBarBadge.foreground": "#FFFFFF",
    "sideBar.background": "#F0F7F5",
    "sideBar.foreground": "#3F4642",
    "sideBarSectionHeader.background": "#F6FBF7",
    "sideBarSectionHeader.foreground": "#3F4642",
    "statusBar.background": "#F0F7F5",
    "statusBar.foreground": "#3F4642",
    "statusBar.debuggingBackground": "#FFE3EA",
    "statusBar.noFolderBackground": "#F0F7F5",
    "titleBar.activeBackground": "#F0F7F5",
    "titleBar.activeForeground": "#101411",
    "titleBar.inactiveBackground": "#F0F7F5",
    "titleBar.inactiveForeground": "#7C867F",
    "tab.activeBackground": "#F6FBF7",
    "tab.activeForeground": "#101411",
    "tab.inactiveBackground": "#F0F7F5",
    "tab.inactiveForeground": "#7C867F",
    "tab.hoverBackground": "#F0F7F5",
    "tab.editor-group-tab-background": "#F0F7F5",
    "layoutControl.foreground": "#222222",
    "layoutControl.activeForeground": "#FF5D70",
    "editor.selectionHighlightBackground": "#DAEBE1",
    "editor.selectionHighlightBorder": "#DAEBE1",
    "editor.wordHighlightBackground": "#DAEBE1",
    "editor.wordHighlightStrongBackground": "#DAEBE1",
    "editor.wordHighlightBorder": "#DAEBE1",
    "terminal.background": "#F6FBF7",
    "terminal.foreground": "#3F4642",
    "terminal.ansiBlack": "#E0EFE8",
    "terminal.ansiRed": "#FF5D70",
    "terminal.ansiGreen": "#63D8BD",
    "terminal.ansiYellow": "#E6CB36",
    "terminal.ansiBlue": "#7159F9",
    "terminal.ansiMagenta": "#B799FF",
    "terminal.ansiCyan": "#20C093",
    "terminal.ansiWhite": "#F3F9F4",
    "terminal.ansiBrightBlack": "#8B978F",
    "terminal.ansiBrightRed": "#FF7A8A",
    "terminal.ansiBrightGreen": "#8CE7C7",
    "terminal.ansiBrightYellow": "#E9B84A",
    "terminal.ansiBrightBlue": "#9C87FF",
    "terminal.ansiBrightMagenta": "#D9C2FF",
    "terminal.ansiBrightCyan": "#7DE2CB",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "badge.background": "#20C093",
    "badge.foreground": "#FFFFFF",
    "button.background": "#FF5D70",
    "button.hoverBackground": "#FF7A8A",
    "button.foreground": "#FFFFFF",
    "button.secondaryBackground": "#E2EFE6",
    "button.secondaryHoverBackground": "#D5E3DA",
    "button.secondaryForeground": "#3F4642",
    "extensionButton.background": "#FF5D70",
    "extensionButton.foreground": "#FFFFFF",
    "extensionButton.hoverBackground": "#FF7A8A",
    "extensionButton.prominentBackground": "#FF5D70",
    "extensionButton.prominentForeground": "#FFFFFF",
    "extensionButton.prominentHoverBackground": "#FF7A8A",
    "editorWidget.foreground": "#3F4642",
    "editorWidget.background": "#F6FBF7",
    "input.background": "#F6FBF7",
    "input.foreground": "#101411",
    "input.border": "#F6FBF7",
    "input.placeholderForeground": "#7C867F",
    "dropdown.background": "#F6FBF7",
    "dropdown.foreground": "#3F4642",
    "dropdown.border": "#D5E3D8",
    "panel.background": "#F6FBF7",
    "panel.border": "#D5E3D8",
    "panelTitle.activeForeground": "#101411",
    "panelTitle.inactiveForeground": "#7C867F",
    "scrollbarSlider.background": "#C1D0C666",
    "scrollbarSlider.hoverBackground": "#B2C5B988",
    "scrollbarSlider.activeBackground": "#9AB1A399",
    "list.activeSelectionBackground": "#E7F2EE",
    "list.activeSelectionForeground": "#101411",
    "list.hoverBackground": "#E7F2EE",
    "list.hoverForeground": "#101411",
    "list.inactiveSelectionBackground": "#F0F7F5",
    "list.focusBackground": "#E7F2EE",
    "list.highlightForeground": "#FF5D70",
    "notifications.background": "#F6FBF7",
    "notifications.foreground": "#101411",
    "notifications.border": "#D5E3D8",
    "notificationCenterHeader.background": "#F0F7F5",
    "notificationsInfoIcon.foreground": "#7159F9",
    "notificationsWarningIcon.foreground": "#E6CB36",
    "notificationsErrorIcon.foreground": "#FF5D70",
    "quickInput.background": "#F0F7F5",
    "quickInput.foreground": "#3F4642",
    "quickInput.list.focusBackground": "#E7F2EE",
    "focusBorder": "#B8A9F4",
    "textLink.foreground": "#20C093",
    "textLink.hoverForeground": "#20C093",
    "statusBarItem.remoteForeground": "#FFFFFF",
    "statusBarItem.remoteBackground": "#7159F9",
    "keybindingTable.rowsBackground": "#DCEAE4",
    "panelSectionHeader.background": "#DCEAE4",
    "editorGroupHeader.tabsBackground": "#F0F7F5",
    "editorGroupHeader.noTabsBackground": "#F6FBF7"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.line",
        "comment.line.double-slash",
        "punctuation.definition.comment",
        "sss"
      ],
      "settings": {
        "foreground": "#8D9990",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#FF6B82"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "foreground": "#E9556B"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#E9556B"
      }
    },
    {
      "name": "Keyword Operator Assignment",
      "scope": [
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#6F7A73"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#FF8FA8"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#6E59F8"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#6ABA0E"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#20C093"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#9278FF"
      }
    },
    {
      "name": "Class & Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#6150F0"
      }
    },
    {
      "name": "Annotation / Attribute",
      "scope": [
        "entity.other.attribute-name",
        "meta.annotation"
      ],
      "settings": {
        "foreground": "#5BC7A6"
      }
    },
    {
      "name": "Operator / Punctuation",
      "scope": [
        "punctuation",
        "operator"
      ],
      "settings": {
        "foreground": "#738077"
      }
    },
    {
      "name": "Invalid / Error",
      "scope": [
        "invalid",
        "invalid.deprecated",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#B3184C"
      }
    },
    {
      "name": "Property / Object Key",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#20C093"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#FF6B82"
      }
    },
    {
      "name": "Special Variable",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#75DCC0"
      }
    },
    {
      "name": "Module / Namespace",
      "scope": [
        "entity.name.type.module",
        "support.other.module"
      ],
      "settings": {
        "foreground": "#6150F0"
      }
    },
    {
      "name": "text.html.derivative",
      "scope": [
        "text.html.derivative"
      ],
      "settings": {
        "foreground": "#3F4642"
      }
    },
    {
      "name": "HTML Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "meta.tag.html",
        "meta.tag.xml",
        "meta.tag.other"
      ],
      "settings": {
        "foreground": "#FF6B82"
      }
    },
    {
      "name": "HTML Attribute Name",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attribute",
        "meta.tag.xml.attribute"
      ],
      "settings": {
        "foreground": "#20C093"
      }
    },
    {
      "name": "HTML Attribute Value",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.unquoted.html"
      ],
      "settings": {
        "foreground": "#6ABA0E"
      }
    },
    {
      "name": "HTML Punctuation",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "punctuation.definition.tag",
        "punctuation.separator.key-value.html"
      ],
      "settings": {
        "foreground": "#738077"
      }
    },
    {
      "name": "HTML Doctype / Comment",
      "scope": [
        "meta.tag.sgml.doctype",
        "comment.block.html"
      ],
      "settings": {
        "foreground": "#4F5952",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSX/TSX Tag",
      "scope": [
        "entity.name.tag.js",
        "entity.name.tag.tsx",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#FF6B82"
      }
    },
    {
      "name": "JSX Attribute",
      "scope": [
        "entity.other.attribute-name.js",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#20C093"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading",
        "markup.heading.markdown",
        "markup.heading.setext",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#FF6B82",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold",
        "markup.bold.markdown"
      ],
      "settings": {
        "foreground": "#E9556B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic",
        "markup.italic.markdown"
      ],
      "settings": {
        "foreground": "#E9556B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link Text",
      "scope": [
        "string.other.link.title.markdown",
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#20C093",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Link URL",
      "scope": [
        "markup.underline.link",
        "markup.underline.link.image.markdown",
        "markup.underline.link.markdown",
        "markup.link.url.markdown"
      ],
      "settings": {
        "foreground": "#7BBC38"
      }
    },
    {
      "name": "Markdown Code Inline",
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.raw.inline.markdown"
      ],
      "settings": {
        "foreground": "#6E59F8"
      }
    },
    {
      "name": "Markdown Code Block",
      "scope": [
        "markup.fenced_code.block.markdown",
        "markup.raw.block.markdown",
        "fenced_code.block.language",
        "fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#6E59F8"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote",
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#B4D267",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown List / Bullet",
      "scope": [
        "markup.list",
        "markup.list.numbered",
        "markup.list.unnumbered"
      ],
      "settings": {
        "foreground": "#3F4642"
      }
    },
    {
      "name": "Markdown Separator / HR",
      "scope": [
        "meta.separator.markdown"
      ],
      "settings": {
        "foreground": "#4F5952"
      }
    },
    {
      "name": "Markdown Comment (HTML-style)",
      "scope": [
        "comment.block.html.markdown"
      ],
      "settings": {
        "foreground": "#4F5952",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Heading 1 (#)",
      "scope": [
        "punctuation.definition.heading.markdown",
        "heading.1.markdown",
        "markup.heading.markdown",
        "markup.heading.setext.markdown",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#FF6B82",
        "fontStyle": "bold"
      }
    },
    {
      "name": "List Bullet (-)",
      "scope": [
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#FF6678"
      }
    },
    {
      "name": "List Bullet (-)",
      "scope": [
        "markup.list.unnumbered.markdown",
        "markup.list.numbered.markdown"
      ],
      "settings": {
        "foreground": "#3F4642"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "meta.object-literal.key",
        "meta.object.member",
        "meta.objectliteral",
        "support.type.property-name",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#20C093"
      }
    },
    {
      "name": "Object Property",
      "scope": [
        "variable.other.property",
        "variable.other.property.js",
        "variable.other.object.property",
        "support.variable.property.dom",
        "meta.property.object.js"
      ],
      "settings": {
        "foreground": "#20C093"
      }
    },
    {
      "name": "Method (dot chain)",
      "scope": [
        "support.function",
        "variable.function",
        "variable.function.js",
        "entity.name.function.member",
        "meta.method-call"
      ],
      "settings": {
        "foreground": "#9278FF"
      }
    },
    {
      "name": "Object variable (req, res, fs, helper, etc.)",
      "scope": [
        "variable.other.object.js",
        "variable.other.object.js.jsx"
      ],
      "settings": {
        "foreground": "#20C093"
      }
    },
    {
      "name": "Promise",
      "scope": [
        "support.class.promise.js",
        "support.class.promise.ts"
      ],
      "settings": {
        "foreground": "#75DCC0"
      }
    },
    {
      "name": "Diff Header (from-file)",
      "scope": [
        "meta.diff.header.from-file",
        "punctuation.definition.from-file.diff"
      ],
      "settings": {
        "foreground": "#FF6B82",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Diff Header (to-file)",
      "scope": [
        "meta.diff.header.to-file",
        "punctuation.definition.to-file.diff"
      ],
      "settings": {
        "foreground": "#6ABA0E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Diff Range / Index Line",
      "scope": [
        "meta.diff.range",
        "meta.diff.index"
      ],
      "settings": {
        "foreground": "#6150F0"
      }
    },
    {
      "name": "Diff Added Line",
      "scope": [
        "markup.inserted.diff"
      ],
      "settings": {
        "foreground": "#6ABA0E"
      }
    },
    {
      "name": "Diff Removed Line",
      "scope": [
        "markup.deleted.diff"
      ],
      "settings": {
        "foreground": "#FF6B82"
      }
    },
    {
      "name": "Diff Changed Line",
      "scope": [
        "markup.changed.diff"
      ],
      "settings": {
        "foreground": "#D1DF52"
      }
    },
    {
      "name": "Diff Context Line",
      "scope": [
        "meta.diff.context",
        "meta.diff"
      ],
      "settings": {
        "foreground": "#738077"
      }
    },
    {
      "name": "Diff Range Punctuation",
      "scope": [
        "punctuation.definition.range.diff"
      ],
      "settings": {
        "foreground": "#6150F0"
      }
    },
    {
      "name": "Diff Meta Info",
      "scope": [
        "meta.diff.header",
        "meta.separator.diff"
      ],
      "settings": {
        "foreground": "#6E59F8"
      }
    },
    {
      "name": "Diff Inserted Punctuation",
      "scope": [
        "punctuation.definition.inserted.diff"
      ],
      "settings": {
        "foreground": "#20C093"
      }
    },
    {
      "name": "CSS",
      "scope": [
        "support.function.misc.css",
        "meta.function.variable.css"
      ],
      "settings": {
        "foreground": "#9278FF"
      }
    }
  ]
}
